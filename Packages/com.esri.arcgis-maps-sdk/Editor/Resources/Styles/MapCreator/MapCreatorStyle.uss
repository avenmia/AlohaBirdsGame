.main-foldout > .unity-foldout__toggle .unity-toggle__input {
	background-color: var(--unity-colors-inspector_titlebar-background);
	border-bottom-color: #2D2D2D;
	border-bottom-width: 1px;
	border-top-color: black;
	border-top-width: 1px;
	padding-top: 3px;
	padding-bottom: 3px;
}

.main-foldout > #unity-content {
	background-color: var(--unity-colors-window-background);
	margin-bottom: 8px;
	margin-left: 0px;
	margin-top: 4px;
}

.main-foldout .unity-base-field__label {
	min-width: 180px;
}

.empty-foldout .unity-base-field__label {
	min-width: 176px;
}

.sub-foldout .unity-toggle {
	margin-left: -2px;
}

.sub-foldout .unity-foldout__content {
	margin-left: 10px;
}

.sub-foldout .unity-label {
	min-width: 185px;
}

.layout-row {
	flex-direction: row;
	overflow: hidden;
}

.layout-row Label {
	align-self: center;
	min-width: 182px;
}

#button-transfer-to-camera {
	margin: 0px;
}

.tool-holder {
	margin: 10px;
	margin-bottom: 1px;
}

.create-button-holder {
	align-items: flex-start;
	border-top-width: 1px;
	border-top-color: black;
	flex-direction: row;
	justify-content: flex-start;
}

.button-create {
	flex: 1;
	height: 30px;
	max-width: 260px;
	margin: 20px;
	margin-top: 20px;
	margin-bottom: 20px;
}

.hyperlink-text {
	color: #4B79F0;
	cursor: link;
	font-size: 12px;
	margin-top: 2px;
	margin-bottom: 2px;
}

.hyperlink-text:hover {
	color: #6E93F1;
}

.white-text-small {
	-unity-text-overflow-position: start;
	white-space: normal;
}

.white-text-small#sdk-title {
	-unity-font-style: bold;
}

.button-file-selector {
	width: 28px;
}

.kebab-button {
	background-color: transparent;
	border-width: 0;
	font-size: 16px;
	margin-left: 8px;
	margin-right: 0px;
	rotate: 90deg;
}

.toolbar-box {
	align-items: stretch;
	border-radius: 4px;
	display: flex;
	flex-direction: row;
	flex-shrink: 0;
	flex-wrap: wrap;
	margin: 10px 2px 10px 2px;
	overflow: hidden;
	padding: 0 2px 0 2px;
}

.dark .toolbar-box {
	background-color: #1A1A1A;
}

.light .toolbar-box {
	background-color: #B2B2B2;
}
