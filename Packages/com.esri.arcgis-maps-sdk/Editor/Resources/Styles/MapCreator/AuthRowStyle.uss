Foldout#auth-config-foldout {
	margin-bottom: -5px;
}

Foldout#auth-config-foldout > .unity-foldout__toggle .unity-toggle__input {
	margin-left: 3px;
	padding-bottom: 3px;
	padding-top: 3px;
	border-bottom-width: 1px;
	border-top-width: 1px;
}

.dark Foldout#auth-config-foldout > .unity-foldout__toggle .unity-toggle__input {
	background-color: #303030;
	border-bottom-color: #2D2D2D;
	border-top-color: black;
}

.light Foldout#auth-config-foldout > .unity-foldout__toggle .unity-toggle__input {
	background-color: #E4E4E4;
	color: #383838;
}

Foldout#auth-config-foldout .unity-foldout__content {
	margin-bottom: 8px;
	margin-right: 20px;
}

Foldout#auth-config-foldout TextField {
	margin-top: 1px;
}
